<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:tns="http://www.geography.com/countries"
           xmlns:xsd="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.geography.com/countries" elementFormDefault="qualified">

    <xsd:element name="CountryGKRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Name" type="xsd:string" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="GK">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Name" type="xsd:string" />
                <xsd:element name="Capital" type="xsd:string" />
                <xsd:element name="Currency" type="xsd:string" />
                <xsd:element name="president" type="xsd:string" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

</xs:schema>